    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081556</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081556</ArticleId>
    </ArticleIdList>
    <History>
      <PubDate PubStatus="received">
        <Year>2023</Year>
        <Month>06</Month>
        <Day>28</Day>
      </PubDate>
      <PubDate PubStatus="revised">
        <Year>2023</Year>
        <Month>08</Month>
