    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081501</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081501</ArticleId>
    </ArticleIdList>
    <History>
      <PubDate PubStatus="received">
        <Year>2023</Year>
        <Month>07</Month>
        <Day>07</Day>
      </PubDate>
      <PubDate PubStatus="revised">
        <Year>2023</Year>
        <Month>07</Month>
        <Day>21</Day>
