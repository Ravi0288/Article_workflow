    <ArticleTitle>The Impact of Agricultural Machinery Socialization Services on the Scale of Land Operation: Evidence from Rural China</ArticleTitle>
    <ELocationID EIdType="pii">1594</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Siyu</FirstName>
        <MiddleName/>
        <LastName>Yang</LastName>
      </Author>
      <Author>
        <FirstName>Feng</FirstName>
        <MiddleName/>
        <LastName>Zhang</LastName>
        <AffiliationInfo>
          <Affiliation>College of Applied Science and Technology, Beijing Union University, Beijing 100012, China</Affiliation>
        </AffiliationInfo>
      </Author>
    </AuthorList>
    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081594</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081594</ArticleId>
    </ArticleIdList>
    <History>
      <PubDate PubStatus="received">
        <Year>2023</Year>
        <Month>07</Month>
        <Day>06</Day>
      </PubDate>
      <PubDate PubStatus="revised">
        <Year>2023</Year>
        <Month>07</Month>
        <Day>28</Day>
