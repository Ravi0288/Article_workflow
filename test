      <Author>
        <FirstName>Felizitas</FirstName>
        <MiddleName/>
        <LastName>Winkhart</LastName>
      </Author>
      <Author>
        <FirstName>Kurt-Jürgen</FirstName>
        <MiddleName/>
        <LastName>Hülsbergen</LastName>
