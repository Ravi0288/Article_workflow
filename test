    <ArticleTitle>Does the Development of Digital Inclusive Finance Promote the Construction of Digital Villages?&mdash;An Empirical Study Based on the Chinese Experience</ArticleTitle>
    <ELocationID EIdType="pii">1616</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Chengkai</FirstName>
        <MiddleName/>
        <LastName>Zhang</LastName>
      </Author>
      <Author>
        <FirstName>Yu</FirstName>
        <MiddleName/>
        <LastName>Li</LastName>
      </Author>
      <Author>
        <FirstName>Lili</FirstName>
        <MiddleName/>
        <LastName>Yang</LastName>
      </Author>
      <Author>
        <FirstName>Zheng</FirstName>
        <MiddleName/>
        <LastName>Wang</LastName>
        <AffiliationInfo>
          <Affiliation>Beijing Academy of Science and Technology, Beijing 100089, China</Affiliation>
        </AffiliationInfo>
      </Author>
    </AuthorList>
    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081616</ArticleId>
