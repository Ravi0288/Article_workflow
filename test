    <ArticleTitle>VGNet: A Lightweight Intelligent Learning Method for Corn Diseases Recognition</ArticleTitle>
    <ELocationID EIdType="pii">1606</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Xiangpeng</FirstName>
        <MiddleName/>
        <LastName>Fan</LastName>
      </Author>
      <Author>
        <FirstName>Zhibin</FirstName>
        <MiddleName/>
        <LastName>Guan</LastName>
        <AffiliationInfo>
          <Affiliation>Agricultural Information Institute, Chinese Academy of Agricultural Sciences, Beijing 100081, China</Affiliation>
        </AffiliationInfo>
      </Author>
    </AuthorList>
    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081606</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081606</ArticleId>
