        <LastName>Florczak</LastName>
      </Author>
      <Author>
        <FirstName>Barbara</FirstName>
        <MiddleName/>
        <LastName>Zajdel</LastName>
      </Author>
      <Author>
        <FirstName>Iwona</FirstName>
        <MiddleName/>
        <LastName>Skrzecz</LastName>
      </Author>
    </AuthorList>
    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081582</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081582</ArticleId>
    </ArticleIdList>
    <History>
      <PubDate PubStatus="received">
        <Year>2023</Year>
        <Month>07</Month>
        <Day>05</Day>
      </PubDate>
      <PubDate PubStatus="revised">
        <Year>2023</Year>
