    <ArticleTitle>Evaluation of Environmental and Economic Performance of Crop Production in Relation to Crop Rotation, Catch Crops, and Tillage</ArticleTitle>
    <ELocationID EIdType="pii">1539</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Alberts</FirstName>
        <MiddleName/>
        <LastName>Auzins</LastName>
      </Author>
      <Author>
        <FirstName>Ieva</FirstName>
        <MiddleName/>
        <LastName>Leimane</LastName>
        <AffiliationInfo>
          <Affiliation>Institute of Agricultural Resources and Economics, LV-1039 Riga, Latvia</Affiliation>
        </AffiliationInfo>
      </Author>
      <Author>
        <FirstName>Agnese</FirstName>
        <MiddleName/>
        <LastName>Krievina</LastName>
      </Author>
      <Author>
        <FirstName>Inga</FirstName>
        <MiddleName/>
        <LastName>Morozova</LastName>
      </Author>
      <Author>
        <FirstName>Andris</FirstName>
        <MiddleName/>
        <LastName>Miglavs</LastName>
      </Author>
      <Author>
        <FirstName>Peteris</FirstName>
        <MiddleName/>
        <LastName>Lakovskis</LastName>
      </Author>
