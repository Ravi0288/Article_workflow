    <ArticleTitle>The Emission from Rabbits Breeding in Slovakia</ArticleTitle>
    <ELocationID EIdType="pii">1468</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Kristína</FirstName>
        <MiddleName/>
        <LastName>Tonhauzer</LastName>
        <AffiliationInfo>
