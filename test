    <ArticleTitle>A Century of Vitamin E: Early Milestones and Future Directions in Animal Nutrition</ArticleTitle>
    <ELocationID EIdType="pii">1526</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Yauheni</FirstName>
        <MiddleName/>
        <LastName>Shastak</LastName>
        <AffiliationInfo>
          <Affiliation>Nutrition & Health Division, BASF SE, 67063 Ludwigshafen am Rhein, Germany</Affiliation>
        </AffiliationInfo>
      </Author>
      <Author>
        <FirstName>Ute</FirstName>
        <MiddleName/>
        <LastName>Obermueller-Jevic</LastName>
      </Author>
      <Author>
        <FirstName>Wolf</FirstName>
        <MiddleName/>
        <LastName>Pelletier</LastName>
      </Author>
    </AuthorList>
    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081526</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081526</ArticleId>
