    <ArticleTitle>Design and Test of Single-Disc Opener for No-Till Planter Based on Support Cutting</ArticleTitle>
    <ELocationID EIdType="pii">1635</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Guangyuan</FirstName>
        <MiddleName/>
        <LastName>Zhong</LastName>
      </Author>
      <Author>
        <FirstName>Hongwen</FirstName>
        <MiddleName/>
        <LastName>Li</LastName>
        <AffiliationInfo>
          <Affiliation>College of Engineering, China Agricultural University, Beijing 100083, China</Affiliation>
        </AffiliationInfo>
      </Author>
      <Author>
        <FirstName>Jin</FirstName>
        <MiddleName/>
        <LastName>He</LastName>
      </Author>
      <Author>
        <FirstName>Qingjie</FirstName>
        <MiddleName/>
