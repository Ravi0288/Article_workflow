    <ArticleTitle>Use of <i>Spirulina platensis</i> and <i>Curcuma longa</i> as Nutraceuticals in Poultry</ArticleTitle>
    <ELocationID EIdType="pii">1553</ELocationID>
    <Language>EN</Language>
    <AuthorList>
      <Author>
        <FirstName>Adrian</FirstName>
        <MiddleName/>
        <LastName>Bondar</LastName>
      </Author>
      <Author>
        <FirstName>Loredana</FirstName>
        <MiddleName/>
        <LastName>Horodincu</LastName>
