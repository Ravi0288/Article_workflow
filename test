        <LastName>Patanè</LastName>
      </Author>
    </AuthorList>
    <ArticleIdList>
      <ArticleId IdType="pii">agriculture13081569</ArticleId>
      <ArticleId IdType="doi">10.3390/agriculture13081569</ArticleId>
